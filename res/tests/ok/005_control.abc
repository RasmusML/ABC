get_BMI :: (mass_kg: f32, height_meters: f32) -> f32 {
  return mass_kg / (height_meters * height_meters);
}

is_obese :: (mass_kg: f32, height_meters: f32) -> bool {
  return get_BMI(mass_kg, height_meters) >= 25.0; 
}

